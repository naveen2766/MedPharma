#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/theia/MedPharma/myapp/src/Backend/node_modules/.pnpm/nodemon@3.0.2/node_modules/nodemon/bin/node_modules:/home/theia/MedPharma/myapp/src/Backend/node_modules/.pnpm/nodemon@3.0.2/node_modules/nodemon/node_modules:/home/theia/MedPharma/myapp/src/Backend/node_modules/.pnpm/nodemon@3.0.2/node_modules:/home/theia/MedPharma/myapp/src/Backend/node_modules/.pnpm/node_modules:/home/theia/MedPharma/myapp/src/Backend/node_modules:/home/theia/MedPharma/myapp/src/node_modules:/home/theia/MedPharma/myapp/node_modules:/home/theia/MedPharma/node_modules:/home/theia/node_modules:/home/node_modules:/node_modules"
else
  export NODE_PATH="$NODE_PATH:/home/theia/MedPharma/myapp/src/Backend/node_modules/.pnpm/nodemon@3.0.2/node_modules/nodemon/bin/node_modules:/home/theia/MedPharma/myapp/src/Backend/node_modules/.pnpm/nodemon@3.0.2/node_modules/nodemon/node_modules:/home/theia/MedPharma/myapp/src/Backend/node_modules/.pnpm/nodemon@3.0.2/node_modules:/home/theia/MedPharma/myapp/src/Backend/node_modules/.pnpm/node_modules:/home/theia/MedPharma/myapp/src/Backend/node_modules:/home/theia/MedPharma/myapp/src/node_modules:/home/theia/MedPharma/myapp/node_modules:/home/theia/MedPharma/node_modules:/home/theia/node_modules:/home/node_modules:/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin/nodemon.js" "$@"
else
  exec node  "$basedir/../../bin/nodemon.js" "$@"
fi
